{
  "name": "@assetdexter/contracts",
  "version": "1.0.0",
  "scripts": {
    "clean": "powershell -Command \"if (Test-Path artifacts) { Remove-Item artifacts -Recurse -Force }; if (Test-Path cache) { Remove-Item cache -Recurse -Force }\"",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:local": "hardhat run scripts/deploy.ts --network localhost",
    "deploy:linea": "hardhat run scripts/deploy.ts --network lineaSepolia",
    "deploy:base": "hardhat run scripts/deploy.ts --network baseSepolia",
    "chain": "hardhat node",
    "verify": "hardhat verify"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@types/node": "^20.0.0",
    "chai": "^4.3.0",
    "ethers": "^6.10.0",
    "hardhat": "^2.19.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
